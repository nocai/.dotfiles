{
    "suggest.noselect": false,
    "suggest.autoTrigger": "trigger",
    "languageserver": {
        "golang": {
          "command": "gopls",
          "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
          "filetypes": ["go"],
          "initializationOptions": {
                "usePlaceholders": false,
                "completeUnimported": true
          }
        }
      }
}
