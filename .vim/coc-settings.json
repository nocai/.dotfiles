{
    "suggest.noselect": false,
    "suggest.autoTrigger": "trigger",
    "list.insertMappings": {
		"<C-n>": "feedkeys:\\<C-j>",
		"<C-e>": "feedkeys:\\<C-p>"
	},


    "languageserver": {
        "golang": {
          "command": "gopls",
          "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
          "filetypes": ["go"],
          "initializationOptions": {
                "usePlaceholders": false,
                "completeUnimported": true
          }
        }
      },
      "coc.preferences.extensionUpdateCheck": "never"
}
